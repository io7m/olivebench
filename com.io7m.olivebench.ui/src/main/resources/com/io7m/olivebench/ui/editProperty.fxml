<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox stylesheets="@olivebench.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" VBox.vgrow="NEVER">
         <children>
            <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="32.0" prefWidth="128.0" text="%ui.metadata.property" />
            <TextField fx:id="propertyField" disable="true" editable="false" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Region maxHeight="-Infinity" minHeight="-Infinity" prefHeight="8.0" />
      <HBox layoutX="10.0" layoutY="10.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0">
         <children>
            <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="32.0" prefWidth="128.0" text="%ui.metadata.value" />
            <TextField fx:id="valueField" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="32.0" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Region layoutX="18.0" layoutY="50.0" maxHeight="-Infinity" minHeight="-Infinity" prefHeight="48.0" />
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <Button fx:id="cancelButton" cancelButton="true" mnemonicParsing="false" onAction="#onCancelSelected" prefHeight="32.0" prefWidth="128.0" text="%ui.discard" HBox.hgrow="NEVER" />
            <Region prefWidth="256.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="confirmButton" defaultButton="true" mnemonicParsing="false" onAction="#onConfirmSelected" prefHeight="32.0" prefWidth="128.0" text="%ui.confirm" HBox.hgrow="NEVER" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</VBox>
